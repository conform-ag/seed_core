    "permissions": [
        {
            "role": "System Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1,
            "submit": 1,
            "cancel": 1
        },
        {
            "role": "Sales Planner",
            "read": 1,
            "write": 1,
            "create": 1,
            "submit": 1,
            "cancel": 1
        },
        {
            "role": "Sales User",
            "read": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC"
