    "issingle": 0,
    "is_submittable": 1,
    "allow_import": 1,
    "track_changes": 1,
    "fields": [
        {
            "fieldname": "naming_series",
            "fieldtype": "Select",
            "label": "Series",
            "options": "STP-.YYYY.-",
            "default": "STP-.YYYY.-",
            "reqd": 1
        },
        {
            "fieldname": "fiscal_year",
            "fieldtype": "Link",
            "label": "Fiscal Year",
            "options": "Fiscal Year",
            "reqd": 1,
            "in_list_view": 1,
            "in_standard_filter": 1
        },
        {
            "fieldname": "column_break_header",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "target_for",
            "fieldtype": "Select",
            "label": "Target For",
            "options": "Company\nCustomer\nTerritory\nSales Person",
            "reqd": 1,
            "in_list_view": 1,
            "in_standard_filter": 1
        },
        {
            "fieldname": "target_entity",
            "fieldtype": "Dynamic Link",
            "label": "Target Entity",
            "options": "target_for",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "currency",
            "fieldtype": "Link",
            "label": "Currency",
            "options": "Currency",
            "reqd": 1
        },
        {
            "fieldname": "status_section",
            "fieldtype": "Section Break",
            "label": "Status"
        },
        {
            "fieldname": "status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "Draft\nInternal Proposal\nSubmitted to HQ\nHQ Approved\nRejected",
