            "read_only": 1
        },
        {
            "fieldname": "targets_section",
            "fieldtype": "Section Break",
            "label": "Target Items"
        },
        {
            "fieldname": "target_items",
            "fieldtype": "Table",
            "label": "Target Items",
            "options": "Target Item",
            "reqd": 1
        },
        {
            "fieldname": "totals_section",
            "fieldtype": "Section Break",
            "label": "Totals"
        },
        {
            "fieldname": "total_target_qty",
            "fieldtype": "Float",
            "label": "Total Target Qty",
            "read_only": 1
        },
        {
            "fieldname": "total_target_amount",
            "fieldtype": "Currency",
            "label": "Total Target Amount",
            "read_only": 1
        },
        {
            "fieldname": "column_break_totals",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "total_actual_qty",
            "fieldtype": "Float",
            "label": "Total Actual Qty",
            "read_only": 1
        },
        {
            "fieldname": "total_actual_amount",
            "fieldtype": "Currency",
            "label": "Total Actual Amount",
            "read_only": 1
        },
        {
            "fieldname": "achievement_percent",
            "fieldtype": "Percent",
            "label": "Achievement %",
            "read_only": 1
        },
        {
            "fieldname": "amended_from",
            "fieldtype": "Link",
            "label": "Amended From",
            "no_copy": 1,
            "options": "Sales Target Plan",
            "print_hide": 1,
            "read_only": 1
        }
